<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xsi:type="TaskPaneApp" xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xmlns:m="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
  <Id>5c83f902-8261-4227-935e-111122223333</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Mochileros RD</ProviderName>
  <DefaultLocale>es-ES</DefaultLocale>
  <DisplayName DefaultValue="Mochileros RD" />
  <Description DefaultValue="Panel de Mochileros" />
  
  <IconUrl DefaultValue="https://officedev.github.io/Office-Add-in-TaskPane-JS/assets/icon-32.png" />
  <HighResolutionIconUrl DefaultValue="https://officedev.github.io/Office-Add-in-TaskPane-JS/assets/icon-64.png" />
  <SupportUrl DefaultValue="https://georgedeberas.github.io/help" />
  
  <AppDomains>
    <AppDomain>https://georgedeberas.github.io</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://georgedeberas.github.io/mochileros/index.html" />
  </DefaultSettings>
  
  <Permissions>ReadWriteDocument</Permissions>
  
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="MochilerosGroup">
                <Label residue="groupLabel" />
                <Icon>
                  <bt:Image size="16" residue="icon16" />
                  <bt:Image size="32" residue="icon32" />
                  <bt:Image size="80" residue="icon80" />
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label residue="taskpaneButtonLabel" />
                  <Supertip>
                    <Title residue="taskpaneButtonLabel" />
                    <Description residue="taskpaneButtonDesc" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" residue="icon16" />
                    <bt:Image size="32" residue="icon32" />
                    <bt:Image size="80" residue="icon80" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>MochilerosPane</TaskpaneId>
                    <SourceLocation residue="taskpaneUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="icon16" DefaultValue="https://officedev.github.io/Office-Add-in-TaskPane-JS/assets/icon-16.png" />
        <bt:Image id="icon32" DefaultValue="https://officedev.github.io/Office-Add-in-TaskPane-JS/assets/icon-32.png" />
        <bt:Image id="icon80" DefaultValue="https://officedev.github.io/Office-Add-in-TaskPane-JS/assets/icon-80.png" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="taskpaneUrl" DefaultValue="https://georgedeberas.github.io/mochileros/index.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="groupLabel" DefaultValue="Mochileros App" />
        <bt:String id="taskpaneButtonLabel" DefaultValue="Abrir Mochileros" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="taskpaneButtonDesc" DefaultValue="Abre el panel lateral de Mochileros RD" />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
