<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xsi:type="TaskPaneApp" xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xmlns:m="http://schemas.microsoft.com/office/officeappbasictypes/1.0">
  <Id>9999f902-8261-4227-935e-789012349999</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Mochileros RD</ProviderName>
  <DefaultLocale>es-ES</DefaultLocale>
  <DisplayName DefaultValue="Mochileros RD" />
  <Description DefaultValue="Suite de gestión de viajes" />
  
  <IconUrl DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f392.png" />
  <HighResolutionIconUrl DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f392.png" />
  <SupportUrl DefaultValue="https://georgedeberas.github.io/mochileros/index.html" />
  
  <AppDomains>
    <AppDomain>https://georgedeberas.github.io</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://georgedeberas.github.io/mochileros/index.html" />
  </DefaultSettings>
  
  <Permissions>ReadWriteDocument</Permissions>
  
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="TabMochileros">
              
              <Group id="GroupManagement">
                <Label resid="groupMgmtLabel" />
                <Icon>
                  <bt:Image size="16" resid="iconTours" />
                  <bt:Image size="32" resid="iconTours" />
                  <bt:Image size="80" resid="iconTours" />
                </Icon>
                
                <Control xsi:type="Button" id="BtnTours">
                  <Label resid="lblTours" />
                  <Supertip> <Title resid="lblTours" /> <Description resid="descTours" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconTours" />
                    <bt:Image size="32" resid="iconTours" />
                    <bt:Image size="80" resid="iconTours" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlTours" />
                  </Action>
                </Control>

                <Control xsi:type="Button" id="BtnAccounting">
                  <Label resid="lblAccounting" />
                  <Supertip> <Title resid="lblAccounting" /> <Description resid="descAccounting" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconAccounting" />
                    <bt:Image size="32" resid="iconAccounting" />
                    <bt:Image size="80" resid="iconAccounting" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlAccounting" />
                  </Action>
                </Control>

                <Control xsi:type="Button" id="BtnSettings">
                  <Label resid="lblSettings" />
                  <Supertip> <Title resid="lblSettings" /> <Description resid="descSettings" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconSettings" />
                    <bt:Image size="32" resid="iconSettings" />
                    <bt:Image size="80" resid="iconSettings" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlSettings" />
                  </Action>
                </Control>

                <Control xsi:type="Button" id="BtnManual">
                  <Label resid="lblManual" />
                  <Supertip> <Title resid="lblManual" /> <Description resid="descManual" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconManual" />
                    <bt:Image size="32" resid="iconManual" />
                    <bt:Image size="80" resid="iconManual" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlManual" />
                  </Action>
                </Control>
              </Group>

              <Group id="GroupVibes">
                <Label resid="groupVibesLabel" />
                <Icon>
                  <bt:Image size="16" resid="iconPalm" />
                  <bt:Image size="32" resid="iconPalm" />
                  <bt:Image size="80" resid="iconPalm" />
                </Icon>

                <Control xsi:type="Button" id="BtnDestinations">
                  <Label resid="lblDestinos" />
                  <Supertip> <Title resid="lblDestinos" /> <Description resid="descDestinos" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconPlane" />
                    <bt:Image size="32" resid="iconPlane" />
                    <bt:Image size="80" resid="iconPlane" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlTours" />
                  </Action>
                </Control>

                <Control xsi:type="Button" id="BtnRelax">
                  <Label resid="lblRelax" />
                  <Supertip> <Title resid="lblRelax" /> <Description resid="descRelax" /> </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="iconPalm" />
                    <bt:Image size="32" resid="iconPalm" />
                    <bt:Image size="80" resid="iconPalm" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>PaneMochileros</TaskpaneId>
                    <SourceLocation resid="urlTours" />
                  </Action>
                </Control>
              </Group>

              <Label resid="customTabLabel" />
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    <Resources>
      <bt:Images>
        <bt:Image id="iconTours" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f68c.png" /> <bt:Image id="iconAccounting" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f4ca.png" /> <bt:Image id="iconSettings" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/2699.png" /> <bt:Image id="iconManual" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f4d8.png" /> <bt:Image id="iconPlane" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/2708.png" /> <bt:Image id="iconPalm" DefaultValue="https://cdnjs.cloudflare.com/ajax/libs/twemoji/14.0.2/72x72/1f334.png" /> </bt:Images>
      
      <bt:Urls>
        <bt:Url id="taskpaneUrl" DefaultValue="https://georgedeberas.github.io/mochileros/index.html" />
        <bt:Url id="urlTours" DefaultValue="https://georgedeberas.github.io/mochileros/index.html?page=tours" />
        <bt:Url id="urlAccounting" DefaultValue="https://georgedeberas.github.io/mochileros/index.html?page=accounting" />
        <bt:Url id="urlSettings" DefaultValue="https://georgedeberas.github.io/mochileros/index.html?page=settings" />
        <bt:Url id="urlManual" DefaultValue="https://georgedeberas.github.io/mochileros/index.html?page=manual" />
      </bt:Urls>
      
      <bt:ShortStrings>
        <bt:String id="groupMgmtLabel" DefaultValue="Operaciones" />
        <bt:String id="groupVibesLabel" DefaultValue="Inspiración" />
        <bt:String id="customTabLabel" DefaultValue="Mochileros RD" />
        
        <bt:String id="lblTours" DefaultValue="Tours" />
        <bt:String id="lblAccounting" DefaultValue="Contabilidad" />
        <bt:String id="lblSettings" DefaultValue="Ajustes" />
        <bt:String id="lblManual" DefaultValue="Ayuda" />
        <bt:String id="lblDestinos" DefaultValue="Destinos" />
        <bt:String id="lblRelax" DefaultValue="Relax" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="descTours" DefaultValue="Gestionar Tours" />
        <bt:String id="descAccounting" DefaultValue="Ver Hojas y Finanzas" />
        <bt:String id="descSettings" DefaultValue="Configurar Add-in" />
        <bt:String id="descManual" DefaultValue="Ver Manual de Usuario" />
        <bt:String id="descDestinos" DefaultValue="Explorar nuevos destinos" />
        <bt:String id="descRelax" DefaultValue="Modo Zen" />
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
